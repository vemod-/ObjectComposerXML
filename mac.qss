QTreeWidget, CBarWindow QTableWidget, #SidebarFrame QTableWidget, CPatternList QTableWidget
{
	show-decoration-selected: 1;
	selection-background-color: QLinearGradient( x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #D0D0EF, stop: 0.3999 #9999df, stop: 0.4 #8989cf, stop: 1 #4444DF );
	border: none;
	background-color: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop: 0 #F5F9FF, stop: 0.8 #D0D4DA);
	font-size: 11px;
}

CMusicTree QTreeWidget
{
	selection-background-color:rgba(0,0,0,100);
}

CBarWindow QTableWidget
{
	selection-background-color:rgb(130,130,130);
}

QFrame#TreeFrame
{
	background:white;
}

CPropertyWindow QSpinBox, CPropertyWindow QLineEdit, CPropertyWindow QComboBox
{
	font-size:11px;
}

QComboBox QAbstractItemView
{
     font-size:13px;
}

/*
--------------------------------------------------------------------------------------------
*/

QHoverButton
{
	background:none;
	border:none;
}

CLayoutWizard QToolButton,CScoreWizard QToolButton,CStaffsDialog QToolButton,#Toolbox QToolButton,#SidebarFrame QToolButton, QHeaderView:section
{
	background-color: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop: 0 white, stop: 0.39999 #eee, stop: 0.4 #dfdfdf, stop: 1 #c8c8c8 );
	border:1px outset #777;
}

CLayoutWizard QToolButton:checked,#SidebarFrame QToolButton:checked,CScoreWizard QToolButton:checked,CStaffsDialog QToolButton:checked,#Toolbox QToolButton:checked
{
	background-color: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop: 0 #aaa, stop: 0.8 #ddd );
	border:1px inset #777;
}

CLayoutWizard QToolButton:disabled,#SidebarFrame QToolButton:disabled,CScoreWizard QToolButton:disabled,CStaffsDialog QToolButton:disabled,#Toolbox QToolButton:disabled
{
	background-color: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop: 0 #eee, stop: 0.39999 #ddd, stop: 0.4 #cfcfcf, stop: 1 #b8b8b8 );
	border:1px outset #888;
}

#Toolbox QToolButton
{
	border-radius:5px;
}

QHeaderView::section
{
	padding-left:2px;
	border-color:#999;
}

QHeaderView::section:horizontal
{
	border-top:none;
}

QHeaderView::section:horizontal:last, QHeaderView::section:horizontal:only-one
{
	border-right:none;
}

QHeaderView::section:vertical
{
	border-left:none;
	border-top:none;
}


