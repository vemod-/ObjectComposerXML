QSlider::groove:horizontal 
{
	background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 rgba(50, 50, 50, 255), stop:1 rgba(200, 200, 200, 255));
	height: 3px;
	border: 1px inset #555;
	border-radius: 2px;
}

QSlider::handle:horizontal 
{
	background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #fff, stop:1 #999);
	border: 1px solid #888;
	width: 9px;margin-top: -4px;
	margin-bottom: -4px;
	border-radius: 5px;
}

* [transparent="true"]
{
	background:none;
	border:none;
}

