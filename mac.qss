QMenu
{
	background-color:rgba(0,0,0,70%);
	border:2px outset #666;
	border-radius:6px;
	margin:0px;
	padding:4px;
}

QMenu::item
{
	color:white;
}

QMenu::item:disabled
{
	color:grey;
}

QMenu::item:selected:enabled
{
	background-color:QLinearGradient( x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #a0a0EF, stop: 0.3999 #6666df, stop: 0.4 #5959cf, stop: 1 #1111DF );
}

QMenu::separator
{
	margin:6px;
	height:2px;
	background:#444;
}

QFrame#NavFrame
{
	background-color: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop: 0 #aaa, stop: 0.39999 #666, stop: 0.4 #555, stop: 1 #383838);
}

LayoutViewXML, OCNoteToolbox, CLayoutWizard QGraphicsView, #PnoScroll
{
	border: none;
	border-bottom-width: 1px;
	border-bottom-color: #444444;
	border-bottom-style: solid;
	background-color: QLinearGradient( x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #333, stop: 0.2 #444 );
}

OCPiano
{
        border: none;
}

OCNoteToolbox, #PnoScroll
{
	background-color: QLinearGradient( x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #ddd, stop: 0.3999 #bbb, stop: 0.4 #afafaf, stop: 1 #989898 );
}

CStatusBar
{
        background-color: QLinearGradient( x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #ddd, stop: 0.3999 #bbb, stop: 0.4 #afafaf, stop: 1 #989898 );
        border:none;
        border-top-width: 1px;
        border-top-color: #666666;
        border-top-style: solid;
}

CToolBar
{
        background-color: QLinearGradient( x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #ddd, stop: 0.3999 #bbb, stop: 0.4 #afafaf, stop: 1 #989898 );
        border:none;
}


