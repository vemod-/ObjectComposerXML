--------------------------------------------------------------------------------------------
*/

#OCPlayControl #seekSlider QSlider::groove:horizontal 
{
	background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 rgba(0, 0, 0, 255), stop:1 rgba(255, 255, 255, 255));
	height: 7px;
	border: 1px inset #555;
	border-radius: 4px;
}

#OCPlayControl #seekSlider QSlider::sub-page:horizontal 
{
	background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 rgba(240, 240, 250, 255), stop:1 rgba(60, 60, 250, 255));
	border: 1px inset #555;
	border-radius: 4px;
}

#OCPlayControl #seekSlider QSlider::add-page:horizontal 
{
	background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 rgba(100, 100, 100, 255), stop:1 rgba(150, 150, 150, 255));
	border: 1px inset #555;
	border-radius: 4px;
}

#OCPlayControl #seekSlider QSlider::handle:horizontal {
	background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #fff, stop:1 #999);
	border: 1px solid #888;
	width: 11px;
	margin-top: -3px;
	margin-bottom: -3px;
	border-radius: 6px;
}

#OCPlayControl #volumeSlider QSlider::groove:horizontal {
        background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 rgba(0, 0, 0, 255), stop:1 rgba(255, 255, 255, 255));
        height: 5px;
        border: 1px inset #555;
        border-radius: 3px;
}

#OCPlayControl #volumeSlider QSlider::sub-page:horizontal {
        background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 rgba(150, 150, 150, 255), stop:1 rgba(50, 50, 50, 255));
        border: 1px inset #555;
        border-radius: 3px;
}

#OCPlayControl #volumeSlider QSlider::add-page:horizontal {
        background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 rgba(100, 100, 100, 255), stop:1 rgba(150, 150, 150, 255));
        border: 1px inset #555;
        border-radius: 3px;
}

#OCPlayControl #volumeSlider QSlider::handle:horizontal {
background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #fff, stop:1 #999);
        border: 1px solid #888;
        width: 9px;
        margin-top: -3px;
        margin-bottom: -3px;
        border-radius: 5px;
}

#OCPlayControl QFrame#frameOuter
{
	border:1px outset #555;
	border-radius:9;
}

#OCPlayControl QFrame#frameInner
{
	background-color: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop: 0 #aaa, stop: 0.39999 #666, stop: 0.4 #555, stop: 1 #383838);
	border:1px solid #bbb;
	border-radius:8px;border:none;
}

#OCPlayControl QLabel
{
	color:#ddd;
}

